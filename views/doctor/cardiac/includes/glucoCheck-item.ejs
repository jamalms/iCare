<article class="patient-item">

  <div class="patient-item-content" style="text-align: left;">
    <h2 style="text-align: center;"> <%= glucoCheck.patient_name %></h2> 
    
    <p> Patient Number:</br> <%= glucoCheck.patient_id %></p>
    <p> Rsults:</br> <%= glucoCheck.results %></p>
    <p> Time:</br> <%= glucoCheck.timeTaken %></p>
    
  
   
   
   
    <div class="patient-item-actions">
      <a class="btn btn-alt" href="/dr/update-glucoResult/<%= glucoCheck.id%>/edit">Edit</a>
      <form action="/dr/glucoResults/<%= glucoCheck.id %>/delete" method="POST">
        <button class="btn btn-alt">Delete</button>
        <input type="hidden" name="_csrf" value="<%= locals.csrfToken %>">
      </form>
     
      
      
    </div>
      
     
      
      
    </div>
  </div>
</article>
